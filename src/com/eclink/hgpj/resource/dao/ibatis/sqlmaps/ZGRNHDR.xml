	        #house#,
            #ordno#,
	        #poisq#,
	        #blcod#,		        
	        #blksq#,
	        
	        #lstat#,
            #itnbr#,
	        #blcf#,
	        #iqcf#,		        
	        #shqty#,
	        
	        #purum#,
            #sctkji#,
	        #grqty#,
	        #grloc#,		        
	        #grwgt1#,
	        
	        #grwum1#,
            #grwgt2#,
	        #grwum2# )
	]]>
	</insert>



	<!-- 插入操作 -->
	<insert id="insertZgrnbch" parameterClass="ZGRNBCHVO">
	<![CDATA[
	INSERT  INTO ZGRNBCH (
	        GRNNO,
		    GRNLN,
	        GRNBN,
	        SHPNO,
	        SHPLN,
	        SHPBN,
	        VNDNR,
		    HOUSE,
	        ORDNO,
	        POISQ,
	        BLKSQ,
	        
	        ITNBR,
		    SBQTY,
	        GBQTY,
	        LBHNO,
	        PURUM,
	        
	        MFGDT,
		    EXPDT) 	
       VALUES (
       		#grnno#,
            #grnln#,
            #grnbn#,
	        #shpno#,
	        #shpln#,		        
	        #shpbn#,		        
	        #vndnr#,
	        
	        #house#,
            #ordno#,
	        #poisq#,
	        #blksq#,
            #itnbr#,
            
	        #sbqty#,
	        #gbqty#,		        
	        #lbhno#,
	        
	        #purum#,
            #mfgdt#,
	        #expdt# )
	]]>
	</insert>
	<!-- 更新操作 -->
	<update id="updateBch" parameterClass="ZGRNBCHVO">
		<![CDATA[
			UPDATE ZGRNBCH
		]]>
		<dynamic prepend="SET">
			<isNotEmpty prepend="," property="gbqty">
				GBQTY=#gbqty#
			</isNotEmpty>
			<isNotEmpty prepend="," property="mfgdt">
				MFGDT=#mfgdt#
			</isNotEmpty>
			<isNotEmpty prepend="," property="expdt">
				EXPDT=#expdt#
			</isNotEmpty>
			<isNotEmpty prepend="," property="lbhno">
				LBHNO=#lbhno#
			</isNotEmpty>
		</dynamic>
		<![CDATA[
	 		WHERE SHPNO = #shpno# AND GRNLN=#grnln# AND GRNBN=#grnbn# 
		]]>
	</update>

	<!-- 更新操作 -->
	<update id="updateItem" parameterClass="ZGRNITMVO">
		<![CDATA[
			UPDATE ZGRNITM
		]]>
		<dynamic prepend="SET">
			<isNotEmpty prepend="," property="grqty">
				GRQTY=#grqty#
			</isNotEmpty>
			<isNotEmpty prepend="," property="dkqty">
				DKQTY=#dkqty#
			</isNotEmpty>
			<isNotEmpty prepend="," property="grloc">
				GRLOC=#grloc#
			</isNotEmpty>
			<isNotEmpty prepend="," property="lstat">
				LSTAT = #lstat#
			</isNotEmpty>
			<isNotEmpty prepend="," property="grwgt1">
				GRWGT1=#grwgt1#
			</isNotEmpty>
		</dynamic>
		<![CDATA[
			WHERE SHPNO = #shpno# AND GRNLN=#grnln#  
		]]>
	</update>

	<!-- 资源操作删除 -->
	<delete id="deleteBch" parameterClass="ZGRNBCHVO">
		DELETE ZGRNBCH WHERE SHPNO
		= #shpno# AND GRNLN=#grnln# AND GRNBN=#grnbn#
	</delete>

