	<select id="queryModatas" resultMap="MODATAVOMap"
		parameterClass="MODATAVO">
		<![CDATA[ select a.*, b.fitem, b.dptno, b.orqty, b.qtdev ,c.blcf                       
					from modata a                                
					left join momast b
					on a.ordno = b.ordno
					left join itmrva c
					on b.stid = c.stid and a.citem = c.itnbr and a.citr = c.itrv
		]]>
		<dynamic prepend="where">
			<isNotEmpty prepend="AND" property="citwh">
				a.citwh = #citwh#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="ordno">
				a.ORDNO = #ordno#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="fitem">
				b.fitem = #fitem#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="citem">
				a.CITEM = #citem#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="seqnm">
				a.SEQNM = #seqnm#
			</isNotEmpty>
		</dynamic>
		<![CDATA[ order by a.SEQNM ]]>

	</select>

	<select id="queryMbcdrep" resultMap="MBCDREPVOMap"
		parameterClass="MBCDREPVO">
		<![CDATA[ select a.*, b.CUSNM as CDAAYYN, c.BMCBTX , d.bihjtx ,d.BIHLTX as bihltx                        
					from MBCDREP a 
					inner join MBBMCPP c 
					on a.CDAENB = c.BMAENB and a.CDDCCD=c.BMDCCD and a.CDCVNB=c.BMCVNB                                
					left join MBBFREP b
					on a.CDAAYY = b.CUSNO 
					left outer join MBBIREP d 
					on a.CDAITX = d.biaitx and a.CDAAYY=d.bicanb  
		]]>
		<dynamic prepend="where">
			<isNotEmpty prepend="AND" property="cdaenb">
				a.CDAENB = #cdaenb#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="cda3cd">
				a.CDA3CD = #cda3cd#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="cdaayy">
				a.CDAAYY = #cdaayy#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="cddccd">
				a.CDDCCD = #cddccd#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="cdfcnb">
				a.CDFCNB = #cdfcnb#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="cdcvnb">
				a.CDCVNB like
				('%'||#cdcvnb#||'%' )
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="ponum">
				c.BMCBTX like
				('%'||#ponum#||'%' )
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="startDateB">
				<![CDATA[a.CDFCNB in ( select ADFCNB from MBADREP where ADBIDT>=#startDateB# ]]>
				<isNotEmpty prepend="AND" property="endDateB">
					<![CDATA[ADBIDT<=#endDateB#]]>
				</isNotEmpty>
				)
			</isNotEmpty>
			<isEmpty prepend="AND" property="startDateB">

				<isNotEmpty prepend="AND" property="endDateB">
					<![CDATA[a.CDFCNB in ( select ADFCNB from MBADREP where ADBIDT<=#endDateB# )]]>
				</isNotEmpty>

			</isEmpty>
		</dynamic>
		<![CDATA[ order by a.CDCVNB,CDFCNB ]]>

	</select>

	<select id="queryMbbirep" resultClass="String" parameterClass="Map">
		<![CDATA[ select  BIHLTX from  MBBIREP 
		]]>
		<dynamic prepend="where">
			<isNotEmpty prepend="AND" property="biaitx">
				biaitx=#biaitx#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="bicanb">
				bicanb = #bicanb#
			</isNotEmpty>
		</dynamic>
	</select>

	<select id="queryItrvt" resultClass="String" parameterClass="ITMSITVO">
		<![CDATA[ select  ITRVT9  from  ITMSIT where STIDT9=#house# and ITNOT9=#itnot9#
		]]>
	</select>

	<select id="queryItrvtAll" resultClass="ITMSITVO"
		parameterClass="ITMSITVO">
		<![CDATA[ select  *  from  ITMSIT  
		]]>
		<dynamic prepend="where">
			<isNotEmpty prepend="and" property="house">
				STIDT9=#house#
			</isNotEmpty>
			<isNotEmpty prepend="and" property="itnot9">
				ITNOT9=#itnot9#
			</isNotEmpty>
		</dynamic>
	</select>
	<select id="queryItmsitByItnot9Like" resultClass="ITMSITVO"
		parameterClass="ITMSITVO">
		<![CDATA[select temp.itnot9,temp.itrvt9,
		temp.umstt9,temp.blcft9 
		from (select a.* ,rownumber() over() as rowid from 
		(select  *  from  ITMSIT  
		]]>
		<dynamic prepend="where">
			<isNotEmpty prepend="and" property="house">
				STIDT9=#house#
			</isNotEmpty>
			<isNotEmpty prepend="and" property="itnot9">
				ITNOT9 like
				('%'||#itnot9#||'%' )
			</isNotEmpty>
		</dynamic>
		<![CDATA[ ) a) temp where temp.rowid>=0 and temp.rowid <= 10
		]]>
	</select>
	<select id="queryItmrva" resultMap="ITMRVAVOMap" parameterClass="ITMRVAVO">
		<![CDATA[ select  * from  ITMRVA where STID=#house# 
		]]>
		<dynamic>
			<isNotEmpty prepend="AND" property="itnbr">
				ITNBR=#itnbr#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="itrv">
				ITRV = #itrv#
			</isNotEmpty>
		</dynamic>
	</select>
	<select id="querySlqnty" resultMap="SLQNTYVOMap" parameterClass="Map">
		<![CDATA[ select a.*,c.UMSTT9,b.uucalm from slqnty a  left join sldata b on a.house = b.house and a.llocn = b.llocn 
				left outer join ITMSIT c on a.ITNBR=c.ITNOT9  
				where a.house = #warehouse# AND c.STIDT9=#stid#  
		]]>
		<dynamic>
			<isNotEmpty prepend="AND" property="shard">
				b.uucalm = #shard#
			</isNotEmpty>
		</dynamic>
		<dynamic>
			<isNotEmpty prepend="AND" property="location">
				a.llocn = #location#
			</isNotEmpty>
		</dynamic>
		<dynamic>
			<isNotEmpty prepend="AND" property="itnbr">
				a.ITNBR = #itnbr#
			</isNotEmpty>
		</dynamic>
		<dynamic>
			<isNotEmpty prepend="AND" property="lbhno">
				a.LBHNO = #lbhno#
			</isNotEmpty>
		</dynamic>
		<![CDATA[ order by a.fdate ]]>
	</select>

	<select id="querySlqntyForPickList" resultClass="SLQNTYVO"
		parameterClass="Map">
		<![CDATA[
			select a.house, a.itnbr, a.llocn, a.lbhno, a.lqnty, a.FDATE
			from SLQNTY a
			left join SLDATA b
			on a.house = b.house and a.llocn = b.llocn
			where a.house = #house# and a.itnbr = #citem#
			and b.uusalm = '1'
			]]>
		<dynamic>
			<isNotEmpty prepend="and" property="whsub">
				b.uucalm = #whsub#
			</isNotEmpty>
		</dynamic>
				<![CDATA[
			order by FDATE
		]]>
	</select>

	<select id="querySldata" resultClass="SLDATAVO" parameterClass="Map">
		<![CDATA[ select b.* from sldata b  
				where b.house = #warehouse#   
		]]>
		<dynamic>
			<isNotEmpty prepend="AND" property="shard">
				b.uucalm = #shard#
			</isNotEmpty>
		</dynamic>
		<dynamic>
			<isNotEmpty prepend="AND" property="location">
				b.llocn = #location#
			</isNotEmpty>
		</dynamic>
	</select>
	<select id="queryMomast" resultMap="MOMASTVOMap" parameterClass="MOMASTVO">
		<![CDATA[ select  a.*,b.unmsr,b.weght,b.b2cqcd,(a.ORQTY+a.QTDEV) as moqty, (a.ORQTY+a.QTDEV-QTYRC) as mounqty from  MOMAST a left join itmrva b                                     
				on a.stid = b.stid and a.fitem = b.itnbr                       
				and a.itrv = b.itrv 
		 		where a.FITWH=#fitwh#  AND OSTAT IN ('10','40','45','50','55') 
		]]>
		<dynamic>
			<isNotEmpty prepend="AND" property="uusamy">
				a.UUSAMY = #uusamy#
			</isNotEmpty>
		</dynamic>
		<dynamic>
			<isNotEmpty prepend="AND" property="dptno">
				a.DPTNO like
				('%'||#dptno#||'%')
			</isNotEmpty>
		</dynamic>
		<dynamic>
			<isNotEmpty prepend="AND" property="fitem">
				a.FITEM like
				('%'||#fitem#||'%' )
			</isNotEmpty>
		</dynamic>
		
		<dynamic>
			<isNotEmpty prepend="AND" property="ordnoF">
				<iterate property="ordnoF" conjunction="or" open="(" close=")">
					a.ORDNO LIKE ('%'||#ordnoF[]#||'%')
				</iterate>
			</isNotEmpty>
		</dynamic>
			<dynamic>
			<isNotEmpty prepend="AND" property="ostat">
				(a.OSTAT = #ostat#)
			</isNotEmpty>
		</dynamic>
		<dynamic>
			<isNotEmpty prepend="AND" property="ordnodown">
				(a.ORDNO >=
				#ordnodown#)
			</isNotEmpty>
		</dynamic>
		<dynamic>
			<isNotEmpty prepend="AND" property="ordnoup">
               <![CDATA[ (a.ORDNO <= #ordnoup#)  ]]>
			</isNotEmpty>
		</dynamic>

		<dynamic>
			<isNotEmpty prepend="AND" property="startDateB">
				(a.SSTDT >=
				#startDateB#)
			</isNotEmpty>
		</dynamic>
		<dynamic>
			<isNotEmpty prepend="AND" property="endDateB">
               <![CDATA[ (a.SSTDT <= #endDateB#)  ]]>
			</isNotEmpty>
		</dynamic>
		<![CDATA[ order by a.ASTDT desc 
		]]>
	</select>

	<select id="queryMomastByordno" resultMap="MOMASTVOMap"
		parameterClass="MOMASTVO">
		<![CDATA[ select  a.*,'' AS unmsr,0 AS weght,'' AS b2cqcd,0 as moqty, 0 as mounqty  from  MOMAST a  
		 		where a.ORDNO=#ordno# 
		]]>

	</select>

	<select id="queryMomastBystate" resultMap="MOMASTVOMap"
		parameterClass="MOMASTVO">
		<![CDATA[ select  a.*,b.unmsr,b.weght,b.b2cqcd,(a.ORQTY+a.QTDEV) as moqty, (a.ORQTY+a.QTDEV-QTYRC) as mounqty  from  MOMAST a  left join ITMRVA b                                     
				on a.stid = b.stid and a.fitem = b.itnbr                       
				and a.itrv = b.itrv 
		 		where a.ORLCD=5 AND a.OSTAT in ('10', '40', '50')  
		 		
		]]>
		<dynamic>
			<isNotEmpty prepend="AND" property="ordno">
               <![CDATA[ (a.ORDNO = #ordno#)  ]]>
			</isNotEmpty>
		</dynamic>
		<dynamic>
			<isNotEmpty prepend="AND" property="fitwh">
               <![CDATA[ (a.FITWH = #fitwh#)  ]]>
			</isNotEmpty>
		</dynamic>

	</select>

	<select id="queryMoporf" resultClass="MOPORFVO" parameterClass="MOPORFVO">
		<![CDATA[ select  a.*,c.SCTKJI from  MOPORF  a  left outer join SCHRCP c on 
		       c.ORDRJI = a.PONR and c.PISQJI = a.PISQ
			   and c.BKSQJI = a.BKSQ 		        
		 		where a.PONR like 'P%' 
		]]>
		<dynamic>
			<isNotEmpty prepend="and" property="monr">
				a.MONR=#monr#
			</isNotEmpty>
			<isNotEmpty prepend="and" property="opsq">
				a.OPSQ=#opsq#
			</isNotEmpty>
			<isNotEmpty prepend="and" property="ponr">
				a.PONR=#ponr#
			</isNotEmpty>
		</dynamic>

	</select>

	<select id="queryMoporfNormal" resultClass="MOPORFVO"
		parameterClass="MOPORFVO">
